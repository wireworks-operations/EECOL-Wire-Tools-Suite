    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "jest": "^29.3.0",
    "jest-environment-jsdom": "^29.3.0",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.9.0",
    "webpack": "^5.75.0",
